{
    "env": {
        "es6": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended"],
    "rules": {
        "camelcase": 0,
        "comma-dangle": 0,
        "consistent-return": 0,
        "curly": 0,
        "dot-notation": 0,
        "eol-last": 0,
        "eqeqeq": 0,
        "global-strict": 0,
        "key-spacing": 0,
        "new-cap": 0,
        "no-alert": 0,
        "no-case-declarations": 0,
        "no-console": 0,
        "no-empty": 0,
        "no-extra-bind": 0,
        "no-fallthrough": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-multi-spaces": 0,
        "no-shadow": 0,
        "no-undef": 2,
        "no-underscore-dangle": 0,
        "no-unused-vars": [1, {"vars": "all", "args": "none"}],
        "no-use-before-define": 0,
        "no-var": 1,
        "quotes": 0,
        "semi": 0,
        "space-infix-ops": 0,
        "space-unary-ops": 0,
        "strict": [2, "never"],
        "yoda": 0
    }
}